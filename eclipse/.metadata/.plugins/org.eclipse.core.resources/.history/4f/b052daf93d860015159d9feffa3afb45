package com.collabmods.techexpansion.blocks;

import com.collabmods.techexpansion.TechExpansion;

import net.minecraft.block.Block;
import net.minecraft.block.BlockContainer;
import net.minecraft.block.material.Material;
import net.minecraft.tileentity.TileEntity;
import net.minecraft.world.World;

public class TileEntityWithInventorytest extends Block {

	public TileEntityWithInventorytest(String unlocalizedName, Material material){
		super(material);
		this.setBlockName(unlocalizedName)
		.setBlockTextureName("techExpansion:test")
		.setCreativeTab(TechExpansion.techExpansion)
		.setHardness(3.0f)
		.setResistance(15.0f)
		.setHarvestLevel("axe", 1);
	}


	
}
