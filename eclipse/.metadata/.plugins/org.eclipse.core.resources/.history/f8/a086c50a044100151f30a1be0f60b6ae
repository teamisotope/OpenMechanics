package com.collabmods.techexpansion.item.tools;

import com.collabmods.techexpansion.TechExpansion;

import net.minecraft.item.ItemPickaxe;

public class ItemAluminumPickaxe extends ItemPickaxe{
	
	public ItemAluminumPickaxe(String unlocalizedName, ToolMaterial material) {
		super(material);
		this.setUnlocalizedName(unlocalizedName);
		this.setTextureName("techExpansion:aluminum_pickaxe");
		this.setCreativeTab(TechExpansion.techExpansion);
	}
}
